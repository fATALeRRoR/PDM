<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Toolbar images -->    
    <BitmapImage x:Key="AddIcon" UriSource="../Images/Res_L_doc_add.png" />
    <BitmapImage x:Key="ViewIcon" UriSource="../Images/Res_L_doc_view.png" />
    <BitmapImage x:Key="EditIcon" UriSource="../Images/Res_L_doc_edit.png" />
    <BitmapImage x:Key="DeleteIcon" UriSource="../Images/Res_L_doc_delete.png" />
    <BitmapImage x:Key="SendIcon" UriSource="../Images/Res_L_doc_mail.png" />
    <BitmapImage x:Key="PrintIcon" UriSource="../Images/Res_L_doc_print.png" />
    <BitmapImage x:Key="ClearFilterIcon" UriSource="../Images/Res_L_doc_filter_delete.png" />
    <BitmapImage x:Key="SettingsIcon" UriSource="../Images/gear.png" />
    <BitmapImage x:Key="AboutIcon" UriSource="../Images/about.png" />
    <BitmapImage x:Key="TodayDateIcon" UriSource="../Images/date_time.png" />
    <BitmapImage x:Key="FileDateIcon" UriSource="../Images/date_file.png" />

    <system:Double x:Key="DefaultButtonWidth">60</system:Double>
    <GridLength x:Key="DefaultMarginSmall">5</GridLength>
    <GridLength x:Key="DefaultMarginBig">10</GridLength>
    
